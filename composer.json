{
    "name": "glebsky/laravel-lang-generator",
    "description": "Searches for multilingual phrases in Laravel project and automatically generates language files for you.",
    "keywords": ["laravel", "language", "translation", "automatic translation"],
    "type": "library",
    "license": "MIT",
    "time": "2021-12-20",
    "authors": [
        {
            "name": "glebsky",
            "email": "laosdeveloper@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^8.0|^9.0",
        "ext-json": "*"
    },
    "autoload": {
        "psr-4": {
            "Glebsky\\LaravelLangGenerator\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Glebsky\\LaravelLangGenerator\\LaravelLangGeneratorServiceProvider"
            ]
        }
    }
}
